{
  "expo": {
    "name": "PathmarkApp",
    "slug": "pathmarkapp",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectID": "2664d365-239d-43e6-b00a-48d9a1be2b1d"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
